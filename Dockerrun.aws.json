{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
	  {
	  	"name": "client",
	  	"image": "bernasconi/multi-client",
	  	"hostname": "client",
	  	"essential": false,
	  	"memory": 128
	  },
	  {
	  	"name": "server",
	  	"image": "bernasconi/multi-server",
	  	"hostname": "api",
	  	"essential": false,
	  	"memory": 128
	  },
	  {
	  	"name": "worker",
	  	"image": "bernasconi/multi-worker",
	  	"hostname": "worker",
	  	"essential": false,
	  	"memory": 128
	  },
	  {
	  	"name": "nginx",
	  	"image": "bernasconi/multi-nginx",
	  	"hostname": "nginx",
	  	"essential": true,
	  	"portMappings": [
	  		{
	  			"hostPost": 80,
	  			"containerPort":80
	  		}
	  	],
	  	"links": ["client", "server"],
	  	"memory": 128
	  }

	]
}